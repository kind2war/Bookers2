<h3>Users</h3>
<table>
  <thead>
    <tr>
      <th width="360px">image</th>
      <th width="360px">name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
    <!--  %= image_tag @users.get_image % -->
      <td><%= image_tag user.get_profile_image(100,100)  %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(user.id) %></td>
    </tr>
      <% end %>
  </tbody>
</table>