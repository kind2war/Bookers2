<h3>Users</h3>
<table>
  <thead>
    <tr>
      <th width="360px">image</th>
      <th width="360px">name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @users.each do |user| %>
      <td><%= user.image %></td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(user.id) %></td>
    </tr>
      <% end %>
  </tbody>
</table>